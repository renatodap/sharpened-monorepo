{
  "name": "@sharpened/courtsync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:ai": "NODE_ENV=development next dev --port 3001",
    "build": "next build",
    "build:ai": "NODE_ENV=production next build && npm run optimize:ai",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ai": "vitest --config vitest.ai.config.ts",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "lint-staged",
    "analyze": "ANALYZE=true npm run build",
    "analyze:ai": "npx lighthouse http://localhost:3001 --view",
    "monitor:ai": "node scripts/monitor-ai.js",
    "optimize:ai": "node scripts/optimize-ai-models.js"
  },
  "dependencies": {
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "@tanstack/react-query": "^5.17.0",
    "zod": "^3.22.4",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "date-fns": "^3.0.6",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "lucide-react": "^0.303.0",
    "sonner": "^1.3.1",
    "framer-motion": "^10.18.0",
    "@tensorflow/tfjs": "^4.17.0",
    "@tensorflow/tfjs-backend-webgl": "^4.17.0",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@mediapipe/pose": "^0.5.1675469404",
    "cloudinary": "^2.0.3",
    "openweather-api-node": "^3.1.0",
    "mapbox-gl": "^3.1.2",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "react-webcam": "^7.2.0",
    "posthog-js": "^1.96.1",
    "resend": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.4.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
    "vitest": "^1.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^24.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.41.0",
    "drizzle-kit": "^0.20.9",
    "tsx": "^4.7.0",
    "lint-staged": "^15.2.0",
    "husky": "^8.0.3",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@next/bundle-analyzer": "^14.1.0",
    "dotenv": "^16.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}