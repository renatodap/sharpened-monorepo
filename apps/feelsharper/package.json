{
  "name": "feelsharper",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:with-embeddings": "npm run generate-embeddings && next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern='__tests__/(components|lib|utils)/'",
    "test:integration": "jest --testPathPattern='__tests__/integration/'",
    "test:performance": "jest --testPathPattern='__tests__/performance/'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --project=visual",
    "test:accessibility": "playwright test --project=accessibility",
    "test:performance:e2e": "playwright test --project=performance",
    "test:all": "npm run test && npm run test:e2e",
    "generate-embeddings": "npx tsx scripts/generate-embeddings.ts",
    "test-assistant": "npx tsx scripts/test-assistant.ts",
    "seed": "npx tsx scripts/seed.ts",
    "docs:expand": "node scripts/docs-expand.js",
    "docs:serve": "cd docs && python -m http.server 8080 || python3 -m http.server 8080",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:types": "npx supabase gen types typescript --local > lib/types/database.ts",
    "verify": "npm run typecheck && npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "@tailwindcss/typography": "^0.5.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.460.0",
    "next": "15.4.5",
    "next-intl": "^3.21.0",
    "openai": "^5.12.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "reading-time": "^1.5.0",
    "recharts": "^2.15.4",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
